model User {
  no          Int       @id @default(autoincrement())
  uuid        String    @unique @default(uuid()) @db.VarChar(100)
  email       String    @unique @db.VarChar(50)
  phoneNumber String    @unique @db.VarChar(20)
  password    String    @default("1234") @db.VarChar(255)
  name        String?   @default("") @db.VarChar(50)
  nickname    String?   @default("") @db.VarChar(50)
  createdDt   DateTime  @default(now())
  updatedDt   DateTime  @updatedAt
  modifiedDt  DateTime?
  deletedDt   DateTime?
  deletedYn   Boolean   @default(false)
  role        Role      @default(USER)
  exter1      String?   @default("") @db.VarChar(50)
  exter2      String?   @default("") @db.VarChar(50)
}
